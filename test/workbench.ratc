
struct vec3 {
	float x;
	float y;
	float z;
}
struct vec2 {
	float x;
	float y;
}
fn bool program(){
	vec2 prova;
	prova.y = 5.0f;
	prova.x = 3.0f;
	vec3 result = createVec3FromVec2(prova, prova);
	echo(result.x);
